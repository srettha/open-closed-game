{
    "compilerOptions": {
        "baseUrl": "./src",
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "dist",
        "resolveJsonModule": true,
        "rootDir": "./src",
        "sourceMap": true,
        "target": "es2017",
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
}
